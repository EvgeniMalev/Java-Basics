import java.util.Scanner;

Note that in the example has an error. If the number is divisible by 2 and
not divisible by 3, it cant display that is divisible by 5

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int a = Integer.parseInt(scanner.nextLine());
        int b = Integer.parseInt(scanner.nextLine());
        int s = -1; 
        
        if (a % 2 == 0 && b % 2 == 0) {
            s = 2;
        } else if (a % 3 == 0 && b % 3 == 0) {
            s = 3;
        } else if (a % 5 == 0 && b % 5 == 0) {
            s = 5;
        }
        
        System.out.println(s);
    }
}
